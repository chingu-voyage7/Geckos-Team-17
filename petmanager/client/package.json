{
  "name": "petmanager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bulma": "^0.7.2",
    "bulma-extensions": "^4.0.0",
    "moment": "^2.23.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "styled-components": "^4.1.2"
  },
  "scripts": {
    "css-build": "node-sass _sass/main.scss css/maim.css --include-path ./src --include-path ./bulma-start src -o src/",
    "css-watch": "npm run css-build --include-path ./src --include-path ./bulma-start src/ -o src/  --watch --recursive",
    "js-build": "babel _javascript --out-dir lib",
    "js-watch": "npm run js-build -- --watch",
    "start": "npm-run-all --parallel css-watch js-start",
    "js-start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
